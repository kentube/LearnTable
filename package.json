{
    "name": "whinepad",
    "version": "2.0.0",
    "babel": {
        "presets": [
            "es2015",
            "react"
        ]
    },
    "scripts": {
        "watch": "watch \"sh scripts/build.sh\" js/source js/__tests__ css/",
        "test": "jest"
    },
    "eslintConfig": {
        "parser": "babel-eslint",
        "plugins": [
            "babel",
            "react"
        ],
        "extends": "eslint:recommended",
        "env": {
            "browser": true,
            "jest": true
        },
        "rules": {
            "no-unused-vars": 0,
            "comma-dangle": [
                2,
                "always-multiline"
            ],
            "react/jsx-uses-react": 1
        }
    },
    "jest00": {
        "scriptPreprocessor": "node_modules/babel-jest",
        "unmockedModulePathPatterns": [
            "node_modules/react",
            "node_modules/react-dom",
            "node_modules/react-addons-test-utils",
            "node_modules/fbjs"
        ]
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-jest": "^23.6.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "browserify": "^16.2.3",
        "classnames": "^2.2.6",
        "javascript-obfuscator": "^0.18.1",
        "jest-cli": "^23.6.0",
        "react": "^16.7.0",
        "react-dom": "^16.7.0",
        "uglifycss": "0.0.29"
    },
    "dependencies": {
        "crypto-js": "^3.1.9-1"
    }
}
